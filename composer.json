{
    "name": "bravepickle/deliveryman-bundle",
    "description": "Symfony bundle for Deliveryman library.",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.1",
        "symfony/config": "^4.1",
        "symfony/event-dispatcher": "^4.1",
        "symfony/http-foundation": "^4.1",
        "symfony/yaml": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "bravepickle/deliveryman": "dev-dev",
        "symfony/http-kernel": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Victor",
            "email": "2bravepickle@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DeliverymanBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeliverymanBundleTest\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/bravepickle/deliveryman"
        }
    ],
    "config": {
        "bin-dir": "bin/"
    }
}
